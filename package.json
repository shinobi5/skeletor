{
  "name": "skeletor",
  "version": "0.0.0",
  "description": "Front-end boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/shinobi5/skeletor"
  },
  "license": "MIT",
  "dependencies": {
    "lit-html": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@pika/web": "^0.4.3",
    "clean-css-cli": "^4.2.1",
    "connect": "^3.6.6",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.3",
    "hankey": "^0.0.3",
    "imagemin-cli": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "prettier": "1.17.0",
    "prompt": "^1.0.0",
    "serve-static": "^1.13.2",
    "slash": "^2.0.0"
  },
  "scripts": {
    "build": "yarn css:minify && node scripts/copy.js",
    "build:registry": "node scripts/build-registry.js",
    "clean:modules": "rm -rf node_modules src/js/pkg",
    "clean:build": "rm -rf build",
    "clean": "yarn clean:build && yarn clean:modules",
    "create-component": "node scripts/create-component.js && yarn build:registry",
    "css:concat": "cat src/css/settings/* src/css/global/* src/css/elements/* src/css/objects/* src/css/utilities/* > src/styles.css",
    "css:minify": "cleancss -o src/styles.css src/styles.css",
    "css:watch": "onchange 'src/css/**/*' -- yarn css:concat",
    "imagemin": "imagemin --out-dir=src/img src/img/**/*.{png,jpg,gif}",
    "prepare": "pika-web --dest src/js/pkg --clean --optimize",
    "serve": "node scripts/server.js",
    "start": "yarn css:concat && npm-run-all --parallel css:watch serve"
  }
}
